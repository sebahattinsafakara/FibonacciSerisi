   "source": [
    "def fibonacci(son):\n",
    "    if son <= 0:\n",
    "        return []\n",
    "    elif son == 1:\n",
    "        return [0]\n",
    "    else:\n",
    "        fib = [0, 1]\n",
    "        while len(fib) < son:\n",
    "            fib.append(fib[-1] + fib[-2])\n",
    "        return fib\n",
    "\n",
    "print(fibonacci(10))\n"
   ]
